#include <stdio.h>
#include <string.h>  

void GetSet(int *arr, int *num) {
 
    printf( "Enter quantity dara[]: " ) ;
    scanf("%d", num);  

    printf( "Enter [%d] elements:\n", *num ) ;
    for (int i = 0; i < *num; i++) {
        scanf( "%d", &arr[i] ) ;  
    }
    
}

int main() {
    int num;
    
    int data[100] ;  

    GetSet( data , &num ) ; 

    printf( "Number of elements: %d\n", num ) ;
    printf( "Entered elements: " ) ;
    for ( int i = 0; i < num; i++ ) {
        printf( "%d ", data[ i ] ) ;  
    }
    printf("\n");

    return 0;
}
